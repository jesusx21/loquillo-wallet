  },
  "homepage": "https://github.com/jesusx21/loquillo-wallet#readme",
  "dependencies": {
    "@hapi/hapi": "^20.0.3",
    "app-root-path": "^3.0.0",
    "better-npm-run": "^0.1.1",
    "camelcase-keys": "^6.2.2",
    "dotenv": "^8.2.0",
    "joi": "^17.3.0",
    "knex": "^0.21.12",
